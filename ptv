#!/usr/bin/python3

import os

from lib import api

credentials = {
    'dev_id': os.getenv('PTV_USER_ID'),
    'api_key': os.getenv('PTV_API_KEY')
}
client = api.Client(**credentials)

print(client.status_check())
print(client.route_types())
print(client.train_routes())

